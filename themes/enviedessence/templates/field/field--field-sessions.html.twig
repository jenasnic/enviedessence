<div{{ attributes.addClass('field-sessions') }}>
    {% include '@enviedessence/component/_underline-title.html.twig' with { title: 'Consultations' } %}
    <div class="sessions">
    {% for item in items %}
        <div
            class="session-wrapper"
            data-aos="zoom-out"
            data-aos-duration="1000"
            data-aos-delay="{{ 300 * loop.index0 }}"
        >
            {{ item }}
        </div>
    {% endfor %}
    </div>
</div>
